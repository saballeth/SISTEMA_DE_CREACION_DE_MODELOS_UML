    "def predict_diagram_from_voice(transcription: str) -> str:\n",
    "    transcription = transcription.lower()\n",
    "    if \"actor\" in transcription and \"use case\" in transcription:\n",
    "        return \"useCaseDiagram\"\n",
    "    elif \"class\" in transcription:\n",
    "        return \"classDiagram\"\n",
    "    return \"unknown\"\n"
   ]
